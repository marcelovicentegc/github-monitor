{
  "name": "github-monitor",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "webpack --config webpack.config.js --watch",
    "makemigrations": "docker-compose run api python manage.py makemigrations",
    "migrate": "docker-compose run api python manage.py migrate",
    "start": "docker-compose up",
    "lint": "eslint assets/js/**/*",
    "fix:lint": "eslint assets/js/**/* --fix",
    "test": "jest --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && npm run lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "homepage": "https://github.com/vintasoftware/github-monitor#readme",
  "dependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "@babel/runtime": "^7.11.2",
    "axios": "^0.20.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-form": "^8.3.6",
    "webpack-bundle-tracker": "^0.4.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "devDependencies": {
    "eslint": "^7.4.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "prettier": "2.1.2"
  }
}
